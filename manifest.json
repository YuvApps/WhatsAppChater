{
  "manifest_version": 3,
  "name": "WhatAppChater",
  "short_name": "Chater",
  "description": "start a new chat in whatsapp by selected phone number",
  "version": "0.0.1",
  "minimum_chrome_version": "38",
  "permissions": [
    "contextMenus", 
    "tabs"],
  "icons": {
    "16": "assets/whatsapp_16.png",
    "32": "assets/whatsapp_32.png",
    "48": "assets/whatsapp_48.png",
    "128": "assets/whatsapp_128.png"
  },
  "background": {"service_worker": "main.js"}
}
